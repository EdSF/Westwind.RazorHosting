@inherits Westwind.RazorHosting.RazorTemplateFolderHost<RazorHostingTests.Person>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>    
    <div>
        @Model.Name @Model.Company @Model.Address.City @Model.Entered
    </div>
    
    <div class="@Model.Name @Model.Company">
        Pseudo Attributes value injection
    </div>

    @{
    for (int i = 0; i < 10; i++)
    {
        Response.WriteLine(i + ".");
        <div>@i</div>
    }

    @
    {
        ResultData = "Hello from the template " + DateTime.Now;
    }        
}
    
@ResultData
</body>
</html>